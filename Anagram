 bool isAnagram(string a, string b){
        
        // Your code here
          // Your code here
   int n= a.size();
   int m= b.size();
    sort(a.begin(),a.end());
    sort(b.begin(),b.end());
    
    if(n>m || m>n)
    return false;
    for(int i=0;i<n;i++)
    {
        if(a[i]!=b[i])
        return false;
        
    }
    return true;
        
    }
